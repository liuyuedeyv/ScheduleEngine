{
  "startInfo": {
    "url": "http://*:5003",
    "module": "M.WFDesigner",
    "serviceCenter": "http://10.50.135.161:8500",
    "authorize": 0,
    "SkyWalking": "10.50.132.88:11800",
    "serviceDiscoveryServer": "http://10.50.132.153:5003",
    "enableApiDoc": 0,
    "enableLogES": 0,
    "enableUrlPrefix": 0,
    "EnableStaticFile": 0
  },
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Trace"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Error"
      }
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File", "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": { "restrictedToMinimumLevel": "Error" }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "log\\fatal.txt",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "1000",
          "restrictedToMinimumLevel": "Fatal"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "log\\error.txt",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "1000",
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "log\\warn.txt",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "1000",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "log\\info_.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "100",
          "restrictedToMinimumLevel": "Information"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "log\\debug_.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "100",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  },
  "SerilogSql": {
    "Using": [ "Serilog.Sinks.File", "Serilog.Sinks.Elasticsearch" ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "logsql\\error.txt",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "1000",
          "restrictedToMinimumLevel": "Error"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "logsql\\warn.txt",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "1000",
          "restrictedToMinimumLevel": "Warning"
        }
      },
      {
        "Name": "File",
        "Args": {
          "formatter": "Serilog.Formatting.Compact.CompactJsonFormatter, Serilog.Formatting.Compact, Version=1.0.0.0, Culture=neutral, PublicKeyToken=24c2f752a8e58a10",
          "path": "logsql\\info_.txt",
          "rollingInterval": "Day",
          "rollOnFileSizeLimit": "True",
          "shared": "True",
          "fileSizeLimitBytes": "10485760",
          "retainedFileCountLimit": "100",
          "restrictedToMinimumLevel": "Debug"
        }
      }
    ]
  }
}